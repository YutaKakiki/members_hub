<%=turbo_frame_tag "team-members" do%>
    <div class="grid grid-cols-1 xl:grid-cols-2 grid-flow-row gap-2 xl:gap-4">
        <%= render partial: 'teams/members/member',collection: @members %> 
    </div>
    <div class="my-10 w-auto">
        <% if @members %>
           <%=paginate(@members)%>
        <% end %>
    </div>
<% end %>

